}

#portal-globalnav, #portal-globalnav li a{
background: rgb(97,104,112); /* Old browsers */
background: -moz-linear-gradient(top, rgba(97,104,112,1) 0%, rgba(43,49,54,1) 100%); /* FF3.6+ */
