/*=========================
       CALLOUTS
==========================*/

html body #content .pullquote{
-webkit-border-radius: 5px;
-moz-border-radius: 5px;
border-radius: 5px;
font-weight:normal!important;
background: rgb(63,76,107)!important; /* Old browsers */
background: -moz-linear-gradient(top,  rgba(63,76,107,1) 0%, rgba(30,47,51,1) 100%)!important; /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(63,76,107,1)), color-stop(100%,rgba(30,47,51,1)))!important; /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  rgba(63,76,107,1) 0%,rgba(30,47,51,1) 100%)!important; /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  rgba(63,76,107,1) 0%,rgba(30,47,51,1) 100%)!important; /* Opera 11.10+ */
background: -ms-linear-gradient(top,  rgba(63,76,107,1) 0%,rgba(30,47,51,1) 100%)!important; /* IE10+ */
background: linear-gradient(top,  rgba(63,76,107,1) 0%,rgba(30,47,51,1) 100%)!important; /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#3f4c6b', endColorstr='#1e2f33',GradientType=0 )!important; /* IE6-9 */
border-left:solid 20px #c7d127!important;
color:white!important;
letter-spacing:0px;
line-height:normal;
padding:10px;
}

html body #content .pullquote a:hover {
color:#75AD0A!important;
}


html body #content .pullquote a {
color:white!important;
border-bottom:none!important;
letter-spacing:0px;
font-size:.8em;
line-height:normal;

}

html body #content .pullquote .callout {
background-color:transparent;
border:0px;
}

html body #content .pullquote .callout a {
text-decoration:none;
border:none;
color:white;
}

